{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": ["off"],
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "no-unused-vars": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
